package main

import (
	"fmt"
	"os"
	"os/exec"

	"github.com/grewwc/go_tools/src/utilsW"
)

func main() {
	ch := make(chan interface{})
	go func() {
		cmd := exec.Command("code", "-w", "./test.txt")
		cmd.Stdin = os.Stdin
		// cmd.Stdout = os.Stdout
		out, err := cmd.Output()
		fmt.Println(out, err)
		ch <- out
	}()
	<-ch
	s := utilsW.ReadString("./test.txt")
	fmt.Println("here", s)
}
